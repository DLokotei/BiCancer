{
  "expo": {
    "name": "BiCancer",
    "slug": "BiCancer",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./features/appInfo/res/icon.png",
    "scheme": "bicancer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./features/appInfo/res/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.devOceanPro.BiCancer"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./features/appInfo/res/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./features/splash/res/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#B44282"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
